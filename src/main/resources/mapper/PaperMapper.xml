<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sunhao.onlineexambackend.mapper.PaperMapper">

    <!-- 根据exam_id查询所有考试 -->
    <select id="getPapersByExamId" resultType="com.sunhao.onlineexambackend.entity.po.Paper">
        SELECT * FROM paper where exam_id = #{exam_id}
    </select>

</mapper>
